{
  "name": "ggj2023_text_adventure",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/index.js",
  "bin": {
    "ggj2023": "./lib/index.js"
  },
  "engines": {
    "node": "16.13.1",
    "npm": "9.4.1"
  },
  "volta": {
    "node": "16.13.1"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "start:windows": "nodemon --watch 'src/**/*.ts' --exec \"npx ts-node\" src/index.ts",
